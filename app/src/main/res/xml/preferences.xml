<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <CheckBoxPreference
        android:key="pref_wifi_instant"
        android:title="@string/wifi_instant"
        android:summaryOff="@string/wifi_instant_summ_off"
        android:summaryOn="@string/wifi_instant_summ_on"
        android:defaultValue="true" />
    <CheckBoxPreference
        android:key="pref_statistics_display"
        android:title="@string/statistics_display"
        android:summaryOff="@string/statistics_display_off"
        android:summaryOn="@string/statistics_display_on"
        android:defaultValue="true" />
</PreferenceScreen>